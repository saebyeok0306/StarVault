function GetCountUnitFromLocation(player: TrgPlayer, loc: TrgLocation, unit: TrgUnit) {
    const read = function(x) {
        return l2v(Bring(player, AtLeast, x, unit, loc));
    };
    return EUDBinaryMax(read, maxv=1700);
}