const s = StringBuffer(1024);
const s2 = StringBuffer(1024);

const SIZE  = 4;
const BUFFER = Db(SIZE*5);

function onPluginStartWorker() {
    randomize();
    SetMemory(BUFFER + 0, SetTo, 111111111);
    SetMemory(BUFFER + 4, SetTo, 222222222);
    SetMemory(BUFFER + 8, SetTo, 333333333);
    SetMemory(BUFFER + 12, SetTo, 0);
    SetMemory(BUFFER + 16, SetTo, 98765);

    setcurpl(0);
    s.insert(0, "BUFFER Address: ");
    foreach(i : py_range(3)) {
        s.appendf("0x{:x} ", BUFFER+i*4);
    }
}

function beforeTriggerExecWorker() {
    SetMemory(0x6509A0, SetTo, 0); //EUD터보
    setcurpl(0);
    s.DisplayAt(0);
    s2.printfAt(1, "Variable Value: {}", dwread_epd(EPD(BUFFER) + 3));
}

function afterTriggerExecWorker() {
}